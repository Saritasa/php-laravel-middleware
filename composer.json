{
    "name": "saritasa/laravel-middleware",
    "type": "library",
    "description": "Saritasa middleware for Laravel",
    "keywords": ["laravel", "middleware"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/http": "5.* || 6.*",
        "illuminate/support": "5.* || 6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4" : {
            "Saritasa\\Middleware\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\Middleware\\MiddlewareServiceProvider"
            ]
        }
    }
}
